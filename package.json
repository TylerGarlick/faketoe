{
  "name": "faketoe",
  "description": "XML to JSON converter",
  "version": "0.0.3",
  "author": "Eran Hammer <eran@hueniverse.com> (http://hueniverse.com)",
  "contributors":[
    "Tom Hughes-Croucher <tom.croucher@gmail.com>"
  ],
  "repository": "git://github.com/walmartlabs/faketoe",
  "main": "index",
  "keywords": [
    "xml",
    "json",
    "conversion"
  ],
  "engines": {
    "node": "0.8.x"
  },
  "dependencies": {
    "node-expat": "2.0.x"
  },
  "devDependencies": {
    "mocha": "1.x.x",
    "chai": "1.x.x",
    "blanket": "1.0.x",
    "travis-cov": "0.2.x"
  },
  "scripts": {
    "test": "make test && make test-cov",
    "blanket": { "pattern": "//^((?!\/node_modules\/)(?!\/test\/).)*$/ig", "onlyCwd": true, "data-cover-flags": { "branchTracking": true } },
    "travis-cov": { "threshold": 100 }
  },
